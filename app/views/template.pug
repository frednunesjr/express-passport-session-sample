doctype html
html(lang="pt-br")
    head
        block htmlHead
            meta(charset="UTF-8")
            meta(http-equiv="X-UA-Compatible", content="IE=edge")
            meta(name="viewport", content="width=device-width, initial-scale=1.0")
            title #{pageTitle}
            script(src="./js/script.js" defer)
            link(rel="stylesheet", href="./css/styles.css")
                
    body
        block htmlBody